{
  "name": "@appsilva/lquent-ai-chat-widget",
  "version": "0.2.1",
  "description": "An embeddable chat widget component for React applications",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    },
    "./embed": "./dist/embed.js",
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "server": "node server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "npm run build:lib && npm run build:embed && npm run build:types",
    "build:types": "tsc --project tsconfig.app.json",
    "build:lib": "vite build",
    "build:embed": "node scripts/build.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "npm": "^11.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  },
  "keywords": [
    "react",
    "chat",
    "widget",
    "typescript",
    "component"
  ],
  "author": "Antonio Silva",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/appsilva/lquent-ai-chat-widget.git"
  },
  "homepage": "https://github.com/appsilva/lquent-ai-chat-widget#readme"
}
